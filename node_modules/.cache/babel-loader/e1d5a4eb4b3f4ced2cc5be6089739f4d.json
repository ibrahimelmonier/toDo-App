') {\n        return this.todos.filter(function (todo) {\n          return !todo.completed;\n        });\n      } else if (this.filter == 'completed') {\n        return this.todos.filter(function (todo) {\n          return todo.completed;\n        });\n      }\n    },\n    showClear: function showClear() {\n      return this.todos.filter(function (todo) {\n        return todo.completed;\n      }).length > 0;\n    }\n  },\n  directives: {\n    focus: {\n      // directive definition\n      inserted: function inserted(el) {\n        el.focus();\n      }\n    }\n  },\n  methods: {\n    addTodo: function addTodo() {\n      if (this.newTodo.trim().length == 0) {\n        return;\n      }\n\n      this.todos.push({\n        id: this.idForTodo,\n        title: this.newTodo,\n        completed: false\n      });\n      this.newTodo = '';\n      this.idForTodo++;\n    },\n    editTodo: function editTodo(todo) {\n      this.beforeEdit = todo.title;\n      todo.editing = true;\n    },\n    doneEdit: function doneEdit(todo) {\n      if (todo.title.trim().length == 0 && todo.title.trim().length == '') {\n        todo.title = this.beforeEdit;\n      }\n\n      todo.editing = false;\n    },\n    cancelEdit: function cancelEdit(todo) {\n      todo.title = this.beforeEdit;\n      todo.editing = false;\n    },\n    removeTodo: function removeTodo(index) {\n      this.todos.splice(index, 1);\n    },\n    checkAllTodos: function checkAllTodos() {\n      this.todos.forEach(function (todo) {\n        return todo.completed = event.target.checked;\n      });\n    },\n    clearCompleted: function clearCompleted() {\n      this.todos = this.todos.filter(function (todo) {\n        return !todo.completed;\n      });\n    }\n  }\n};",{"version":3,"sources":["toDoList.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiDA,eAAA;AACA,EAAA,IAAA,EAAA,UADA;AAEA;AACA;AACA;AACA,EAAA,IALA,kBAKA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,EADA;AAEA,MAAA,SAAA,EAAA,CAFA;AAGA,MAAA,UAAA,EAAA,EA